{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "jsxFragmentFactory": "Fragment",
    "types": [
      "vue/jsx",
      "jest"
    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "declaration": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
    }
  },
  "include": [
    "./src"
  ]
}
