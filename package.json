{
  "name": "naive-pro-components-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" -- && node ./scripts/copyIndex.js",
    "lib:form": "pnpm --filter @39nyx/pro-form build",
    "docs:dev": "pnpm --filter @39nyx/docs dev",
    "docs:build": "pnpm --filter @39nyx/docs docs:build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@39nyx/low-code": "workspace:^",
    "@39nyx/naive-pro-components": "workspace:^",
    "@vicons/ionicons5": "^0.12.0",
    "axios": "^1.7.9",
    "lodash": "^4.17.21",
    "naive-ui": "^2.40.3",
    "pinia": "^2.3.0",
    "sass": "^1.82.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.1",
    "@unocss/eslint-config": "^0.63.6",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.16.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^6.2.6",
    "prettier": "^3.4.2",
    "typescript": "~5.5.4",
    "unocss": "^0.63.6",
    "vite": "6.0.1",
    "vite-plugin-vue-devtools": "^7.6.7",
    "vue-tsc": "^2.1.10"
  }
}
