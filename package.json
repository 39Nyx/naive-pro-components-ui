{
  "name": "naive-pro-components-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" -- && node ./scripts/copyIndex.js",
    "lib:form": "pnpm --filter @39nyx/pro-form build",
    "docs:dev": "pnpm --filter @39nyx/docs dev",
    "docs:build": "pnpm --filter @39nyx/docs docs:build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@39nyx/low-code": "workspace:^",
    "@39nyx/naive-pro-components": "workspace:^",
    "@vicons/ionicons5": "^0.13.0",
    "axios": "^1.10.0",
    "lodash": "^4.17.21",
    "naive-ui": "^2.42.0",
    "pinia": "^3.0.3",
    "sass": "^1.89.2",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.10",
    "@unocss/eslint-config": "66.3.2",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.1",
    "eslint-plugin-vue": "^10.3.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "unocss": "66.3.2",
    "vite": "7.0.2",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^3.0.1"
  }
}
